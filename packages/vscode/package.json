{
  "name": "baseline-impact-copilot-vscode",
  "displayName": "Baseline Impact Copilot",
  "publisher": "your-name",
  "version": "0.1.0",
  "engines": { "vscode": "^1.74.0" },
  "repository": {
    "type": "git",
    "url": "https://github.com/RachelGanonNew/baseline-impact-copilot"
  },
  "files": [
    "extension.js",
    "package.json",
    "README.md"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:css",
    "onLanguage:html"
  ],
  "main": "extension.js",
  "categories": [
    "Linters"
  ],
  "contributes": {
    "configuration": {
      "title": "Baseline Impact Copilot",
      "properties": {
        "bic.enable": { "type": "boolean", "default": true, "description": "Enable diagnostics" }
      }
    }
  },
  "dependencies": {
    "vsce": "^2.15.0"
  }
}
